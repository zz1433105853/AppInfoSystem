<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.appsys.dao.DataDictionary.DataDictionaryMapper">
	<select id="getDataDictionaryByStatus" resultType="DataDictionary">
		select * from data_dictionary where typeCode = 'APP_STATUS'
	</select>
	
	<select id="getDataDictionaryByFlatform" resultType="DataDictionary">
		select * from data_dictionary where typeCode = 'APP_FLATFORM'
	</select>
</mapper>